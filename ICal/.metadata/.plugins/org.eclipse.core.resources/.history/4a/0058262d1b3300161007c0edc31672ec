package ourMethod;

import javax.swing.JTextArea;

public class Clear {
	private JTextArea tTitle_handle, events_handle;

	public Clear(JTextArea tTitle, JTextArea tEvents) {
		tTitle_handle = tTitle;
		events_handle = tEvents
		
	}
	public void ClearEvents (JTextArea Events) {
		events_handle.setText("");
	}

	public void CleartTitle() {
		tTitle_handle.setText(" ");
	}
}
