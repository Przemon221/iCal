package ourMethod;

import javax.swing.JTextArea;

public class Clear {
	private JTextArea tTitle_handle, events_handle;

	public Clear(JTextArea tTitle) {
		tTitle_handle = tTitle;
	}
	public void ClearEvents (JTextArea Events) {
		events_handle.setText(""); = Events;
	}

	public void CleartTitle() {
		tTitle_handle.setText(" ");
	}
}
